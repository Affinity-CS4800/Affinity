
@{
    ViewData["Title"] = "Login";
}

<h1>Login</h1>

<form id="login-form">
    <input type="email" name="username" required="required" placeholder="Email" /><br />
    <input type="text" name="password" required="required" placeholder="Password" /><br />
    <input type="submit" value="Sign In" />
</form>